{
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
  "extends": [
    "standard"
  ],
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "rules": {
    "max-len": [2, 200, 2],
    "quotes": ["error", "double"],
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix" ],
    "no-console": 1
  }
  },
  "globals" : {
    "m": true
  }
}
